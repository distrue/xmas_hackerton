{
  "name": "xmasHackerton",
  "version": "1.0.0",
  "description": "xmas Hackerton",
  "scripts": {
    "eslint-init": "eslint --init",
    "tsc-init": "tsc --init",
    "check-format": "prettier --list-different \"src/**/*.{ts,tsx,css,html,json,md}\" ",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,html,json,md}\" ",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "type-check": "tsc --skipLibCheck",
    "build": "npm run format && npm run lint && npm run type-check && gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "start": "npm run develop"
  },
  "dependencies": {
    "@reach/router": "1.2.1",
    "axios": "^0.19.0",
    "firebase": "7.0.0",
    "gatsby": "2.15.12",
    "gatsby-plugin-manifest": "2.2.29",
    "gatsby-plugin-offline": "3.0.22",
    "gatsby-plugin-react-helmet": "3.1.10",
    "gatsby-plugin-sass": "2.1.14",
    "gatsby-plugin-styled-components": "3.1.11",
    "gatsby-plugin-typescript": "2.1.7",
    "node-sass": "4.12.0",
    "query-string": "6.8.3",
    "rc-slider": "^8.7.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "styled-components": "4.4.0"
  },
  "devDependencies": {
    "@types/node": "12.7.4",
    "@types/node-sass": "4.11.0",
    "@types/query-string": "^6.3.0",
    "@types/rc-slider": "^8.6.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-helmet": "5.0.11",
    "@types/scriptjs": "0.0.2",
    "@types/styled-components": "4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "6.2.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "firebase-tools": "7.4.0",
    "prettier": "1.18.2",
    "typed-css-modules": "0.6.2",
    "typed-scss-modules": "0.0.12",
    "typescript": "3.6.2"
  },
  "author": "distrue"
}
